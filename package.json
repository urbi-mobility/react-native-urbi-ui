{
  "name": "react-native-urbi-ui",
  "version": "0.0.16",
  "description": "Urbi's UI library",
  "license": "Apache-2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.ts,.tsx' --copy-files && tsc || true",
    "predist": "rm -rf ./dist && npm run build && cp package.json LICENSE README.md dist/ ",
    "dist": "cd dist && npm publish",
    "package": "npm run predist && cd dist && npm pack",
    "start": "cd showcase && npm run start",
    "android": "cd showcase && npm run android",
    "ios": "cd showcase && npm run ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/urbi-mobility/react-native-urbi-ui.git"
  },
  "peerDependencies": {
    "formik": "^2.0.3",
    "react": "^16.0.0",
    "react-native": "^0.59.0",
    "react-native-dialogs": "1.0.5",
    "react-native-formik": "^1.7.7",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-linear-gradient": "^2.5.0",
    "react-native-reanimated": "^1.1.0",
    "react-native-typography": "1.4.1",
    "react-native-vector-icons": "6.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/runtime": "^7.6.2",
    "@types/react": "^16.7.0",
    "@types/react-native": "^0.57.0",
    "@types/react-native-vector-icons": "^6.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "formik": "^2.0.3",
    "prettier": "^1.18.2",
    "react": "16.8.3",
    "react-native": "0.59.10",
    "react-native-dialogs": "1.0.5",
    "react-native-formik": "^1.7.7",
    "react-native-gesture-handler": "1.3.0",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-reanimated": "1.1.0",
    "react-native-typography": "^1.4.1",
    "react-native-vector-icons": "^6.6.0",
    "typescript": "^3.6.3"
  }
}
