{
  "name": "react-native-urbi-ui",
  "version": "0.1.11",
  "description": "Urbi's UI library",
  "license": "Apache-2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.ts,.tsx' --copy-files && tsc || true",
    "predist": "rm -rf ./dist && npm run build && cp package.json LICENSE README.md dist/ ",
    "dist": "cd dist && npm publish",
    "package": "npm run predist && cd dist && npm pack",
    "start": "cd showcase && npm run start",
    "android": "cd showcase && npm run android",
    "ios": "cd showcase && npm run ios"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/urbi-mobility/react-native-urbi-ui.git"
  },
